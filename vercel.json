{
    "version": 2,
    "builds": [
      {
        "src": "backend.js",
        "use": "@vercel/node"
      },
      {
        "src": "analysis.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/backend",
        "dest": "/backend.js"
      },
      {
        "src": "/api/analysis",
        "dest": "/analysis.js"
      }
    ]
  }
  